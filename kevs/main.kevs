add node0 : JavascriptNode
add node0.killer : fr.mleduc.kevoree.container.KevoreeInstanceKiller/1.0.0
add node0.ticker : Ticker
add sync : WSGroup
add chan : LocalChannel

attach node0 sync

set node0.killer.key = 'keyex'
set node0.killer.value = 'valueex'

bind node0.killer.trigger chan
bind node0.ticker.tick chan

set sync.master = 'node0'

